<p id="notice"><%= notice %></p>

<h1>Object Books</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @object_books.each do |object_book| %>
      <tr>
        <td><%= link_to 'Show', object_book %></td>
        <td><%= link_to 'Edit', edit_object_book_path(object_book) %></td>
        <td><%= link_to 'Destroy', object_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Object Book', new_object_book_path %>
